<div class="container">
  <h1>My Lists</h1>
  <div class="cards">
    <% @lists.each do |list| %>
      <%= link_to list_path(list) do%>
        <div class="card-product">
          <div class="card-product-infos">
            <h2><%= list.name.capitalize %></h2>
          </div>
        </div>
      <% end %>
    <% end %>
  </div>
  <%= link_to "Add List", new_list_path, class: "btn btn-primary" %>
</div>
